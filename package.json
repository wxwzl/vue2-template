{
  "name": "vue2-ts-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode local",
    "build": "vue-cli-service build",
    "build:dev": "vue-cli-service build --mode development",
    "build:test": "vue-cli-service build --mode test",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint & npm run format",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "analysis": "npm run build --report",
    "format": "npx prettier --write ./",
    "format:css": "npx stylelint src/**/*.css src/**/*.less src/**/*.vue --fix ",
    "inspect": "vue inspect > webpackSetting-output.js"
  },
  "dependencies": {
    "axios": "0.24.0",
    "core-js": "^3.6.5",
    "element-ui": "2.15.6",
    "good-storage": "^1.1.1",
    "js-cookie": "^2.2.1",
    "nprogress": "^0.2.0",
    "register-service-worker": "^1.7.1",
    "vue": "2.6.14",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "3.5.3",
    "vuex": "3.6.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/good-storage": "^1.1.0",
    "@types/jest": "^26.0.14",
    "@types/js-cookie": "^3.0.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "@vue/cli-plugin-babel": "~4.5.7",
    "@vue/cli-plugin-eslint": "~4.5.7",
    "@vue/cli-plugin-pwa": "~4.5.7",
    "@vue/cli-plugin-router": "~4.5.7",
    "@vue/cli-plugin-typescript": "~4.5.7",
    "@vue/cli-plugin-unit-jest": "~4.5.7",
    "@vue/cli-plugin-vuex": "~4.5.7",
    "@vue/cli-service": "~4.5.7",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "compression-webpack-plugin": "^6.0.3",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-html": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "lint-staged": "^10.4.2",
    "multi-env-html-webpack-plugin": "1.0.0-beta.8",
    "node-object-hash": "^2.0.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^2.1.2",
    "regenerator-runtime": "^0.13.9",
    "stylelint": "^13.7.2",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.2.2",
    "svg-sprite-loader": "^5.0.0",
    "svgo": "^1.3.2",
    "tslib": "^2.3.1",
    "typescript": "~4.0.3",
    "vue-template-compiler": "2.6.14",
    "vuex-module-decorators": "^1.0.1",
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,less}": [
      "npx prettier --write"
    ],
    "*.{vue,css,less}": [
      "stylelint --fix"
    ],
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
